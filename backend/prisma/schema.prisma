generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id        Int     @id @default(autoincrement())
  taskName  String
  payload   Json
  priority  String  @default("Medium")
  status    String  @default("pending")
  webhookLog String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([status])
  @@index([priority])
  @@index([createdAt])
}
